{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "InkSchedule 3",
    "version": "0.7.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.sharpmars.inkschedule-3",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "title": "InkSchedule 3",
        "resizable": false,
        "width": 450,
        "height": 600,
        "decorations": false,
        "visible": false,
        "transparent": true,
        "userAgent": "SharpMars-InkSchedule3/0.6.0"
      }
    ],
    "systemTray": {
      "title": "InkSchedule 3",
      "iconPath": "icons/32x32.png",
      "iconAsTemplate": false,
      "menuOnLeftClick": false
    },
    "updater": {
      "active": true,
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDIxMTI3QjVCRDY3QzRGMDkKUldRSlQzeldXM3NTSVIxT0xDcDNPMjRhTlRGcGF4NCtLU2NqMWpkNTJZSVhHY0ZaamZDUGo5eksK",
      "windows": {
        "installMode": "passive",
        "installerArgs": []
      },
      "endpoints": [
        "https://gist.githubusercontent.com/SharpMars/ea7a3cf1379bf3332c8d6e5c8db6f2d9/raw/updater.json"
      ]
    }
  }
}
